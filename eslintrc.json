{
  "root": true,
  "env": {
    "node": true,
    "es2021": true
  },
  "extends": [
    "eslint:recommended",
    "plugin:vue/vue3-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "parserOptions": {
    "ecmaVersion": 12,
    "parser": "@typescript-eslint/parser",
    "sourceType": "module"
  },
  "rules": {},
  "overrides": [
    {
      "files": ["*.astro"],
      "processor": "astro/astro"
    }
  ],
  "plugins": [
    "astro"
  ],
  "settings": {
    "astro/config": {
      "env": "astro3"
    }
  }
}
